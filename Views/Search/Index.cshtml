<link rel="stylesheet" href="../../Content/Styles/search.css"/>

<section class="search-banner-styling search-banner clearfix">
    <div class="container row-gutters-void ">

        <div class="search-container-styling clearfix">
            <div class="location-dd" id="location-dd">
                <select>
                    <option value="0">
                        Location
                    </option>

                    <option value="1">
                        Location 1
                    </option>

                    <option value="2">
                        Location 2
                    </option>

                </select>
            </div>

            <div class="price-from-dd" id="from-dd">
                <select>
                    <option value="0">
                        Price From
                    </option>

                    <option value="1">
                        100000 PKR
                    </option>

                    <option value="2">
                        500000 PKR
                    </option>

                </select>
            </div>

            <div class="price-to-dd" id="to-dd">
                <select>
                    <option value="0">
                        Price To
                    </option>

                    <option value="1">
                        100000 PKR
                    </option>

                    <option value="2">
                        200000 PKR
                    </option>

                </select>
            </div>

            <div class="type-dd" id="type-dd">
                <select>
                    <option value="0">
                        Property Search
                    </option>

                    <option value="1">
                        Apartment
                    </option>

                    <option value="2">
                        House
                    </option>

                </select>
            </div>

            <button class="search-filter-button">
                Search
            </button>
        </div>

    </div>
</section>

<section class="search-property-grid mt-100 clearfix">

<div class="container row-gutters-void">
    <h2 class="heading">Search Property</h2>
</div>

<div class="container row-gutters-void mt-50 searched-property-filling property-listed-items">
<div class="col-sm-12 col-md-4 individual-item clearfix">
    <div class="all-layering">
        @* Image of Property *@
        <img class="img-responsive" src="../../Content/Graphical Assets/Common/h1@2x.png" alt="Home Design 1"/>
        @* Property Name *@
        <div class="col-sm-12 col-md-12  property-details mt-25">
            <h3 class="property-name">Lovelace Greenfield</h3>
        </div>
        @* Property Specification *@
        <div class="col-sm-12 col-md-12  property-details mt-25">
            <div class="col-md-12 col-sm-12  property-specifications">
                <div class="pull-left specification-name"> Area</div>
                <div class="pull-right specification-value"> 34000/M2</div>
            </div>
            <div class="col-md-12 col-sm-12  property-specifications">
                <div class="pull-left specification-name"> Beds</div>
                <div class="pull-right specification-value"> 3 Beds</div>
            </div>
            <div class="col-md-12 col-sm-12 property-specifications">
                <div class="pull-left specification-name"> Bath</div>
                <div class="pull-right specification-value"> 2 Bath</div>
            </div>
            <div class="col-md-12 col-sm-12  property-specifications">
                <div class="pull-left specification-name"> Price</div>
                <div class="pull-right specification-value price"> Rs:10,00000</div>
            </div>
        </div>
        <div class="col-sm-12 col-md-12 property-details mt-10 mb-25">
            <button
                onclick="location.href='/Property/PropertDetail/1';"
                class="property-details-btn">
                View Details
            </button>
        </div>
    </div>

</div>

